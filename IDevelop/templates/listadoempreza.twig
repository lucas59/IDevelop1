{% block title %}Desarrolladores{% endblock %}
{% extends "base.twig" %}
{% block head %}
<link rel="stylesheet" type="text/css" href="{{path_for('Inicio')}}styles/Estilos_generales.css">
{% endblock %}
{% block content %}

<div id="tabla_id">
<h1>Empresas</h1> 
<input placeholder="Buscar..." onkeyup="FiltrarTabla()" type="text" id="buscar" name="">
  <table id="tabla" class="table">  
    <tbody>
  {% for emp in Empresas %}
  <tr onclick="window.location='{{path_for('Inicio')}}Usuario/PerfilE?email={{ emp.email }}'" class="fila">
  <td>
    <button  style="background-image: url(data:image/png;base64,{{emp.contenido}})" id="boton" type="button" class="dropbtn user" data-toggle="dropdown" data-display="static" aria-haspopup="true" aria-expanded="false">
    </button>
        {{emp.nombre}}
  </td>
    <td>{{ emp.email }}</td>
    <td>{{ emp.telefono }}</td>
  </tr>
  {% endfor %}
</tbody>
</table>
{% endblock %}

{% block script %}
<script type="text/javascript" src="{{path_for('Inicio')}}scripts/filtrarTabla.js"></script>
{%endblock%}